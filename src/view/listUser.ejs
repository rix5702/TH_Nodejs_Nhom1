<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th>STT</th>
            <th>Họ tên</th>
            <th>Tài khoản</th>
            <th>Giới tính</th>
            <th>Email</th>
            <th>Địa chỉ</th>
            <th>Quyền</th>
            <th>Hiệu chỉnh</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i<data.rows.length; i++) { %>
        <tr>
            <td><%= i+1 %></td>
            <td><%= data.rows[i].hoten %></td>
            <td><%= data.rows[i].account %></td>
            <td><%  if (data.rows[i].gender === 1) 
                        { %> nam <% } 
                    else if (data.rows[i].gender === 2) 
                        { %> nữ <% } 
                    else 
                        { %> Unknown<% } %>
            </td>
            <td><%= data.rows[i].email %></td>
            <td><%= data.rows[i].address %></td>
            <td><%= data.rows[i].groupid %></td>
            <td>
              
                <button class="btn btn-warning">
                   <a href="/update/<%= data.rows[i].stt %>"><i class="fas fa-pencil-alt"></i></a> 
                </button>
                <button class="btn btn-danger">
                   <a href="/delete/<%= data.rows[i].stt %>"> <i class="fas fa-trash"></i></a>
                </button>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>
