<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th>STT</th>
            <th>Họ tên</th>
            <th>Tài khoản</th>
            <th>Giới tính</th>
            <th>Email</th>
            <th>Địa chỉ</th>
            <th>Quyền</th>
            <th>Hiệu chỉnh</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i<data.rows.length; i++) { %>
        <tr>
            <td><%= i+1 %></td>
            <td><%= data.rows[i].fullname %></td>
            <td><%= data.rows[i].username %></td>
            <td><%= data.rows[i].sex %></td>
            <td><%= data.rows[i].email %></td>
            <td><%= data.rows[i].address %></td>
            <td><%= data.rows[i].status %></td>
            <td>
                <a href="/detail-user/<%= data.rows[i].username %>" class="btn btn-success">
                    <i class="fas fa-eye"></i>
                </a>
                <button class="btn btn-warning">
                    <i class="fas fa-pencil-alt"></i>
                </button>
                <button class="btn btn-danger">
                    <i class="fas fa-trash"></i>
                </button>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>
<div>
</div>

<!-- Bootstrap CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">

